<!DOCTYPE html>
<html>
  <head>
    {% include 'seo/tracking/googleanalytics.njk' %}
    {% include 'seo/metatags.njk' %}
    {% block extra_css %}{% endblock %}
    <link rel="stylesheet" href="/css/{{ projectFilesName }}.libs.css">
    <link rel="stylesheet" href="/css/main.css">
    <link runat="server" rel="shortcut icon" href="/img/favicon/rv-favicon.png" type="image/x-icon"/>
    <link runat="server" rel="icon" href="/img/favicon/rv-favicon.png" type="image/ico"/>
    <title>{% block title %}{% endblock %} | Rivers Legal</title>
    {% include 'seo/tracking/facebookpixel.njk' %}
  </head>
  <body>
    <div id="page">
      {% if section == 'en/index' or section == 'index' or section  == 'en/about-us' or section == 'en/practice-areas' or section == 'en/members' or section == 'en/contact' or section == 'en/careers' or section == 'en/practice-areas/civil-and-commercial-litigation' or section == 'en/practice-areas/constitutional-amparo-and-administrative-proceedings' or section == 'en/practice-areas/corporate' or section == 'en/practice-areas/foreign-investment' or section == 'en/practice-areas/immigration' or section == 'en/practice-areas/intellectual-property' or section == 'en/practice-areas/labor-and-employment' or section == 'en/practice-areas/mergers-and-acquisitions' or section == 'en/practice-areas/real-estate' or section == 'en/members/francisco-javier-padilla-mitre' or section == 'en/members/jaime-eduardo-martinez-terrazas' or section == 'en/members/Kristopher-ruben-ortiz-hernandez' or section == 'en/members/manuel-victorio-ortiz-chevaile' or section == 'en/members/mauricio-cristopher-arevalo-alcalde' or section == 'en/members/montserrat-del-rio-castaneda' or section == 'en/members/valeria-larrondo-tafoya' or section == 'en/members/vivian-ramirez-valladolid' %}
        {% include 'partials/navbar-en.njk' %}
      {% else %}
        {% include 'partials/navbar.njk' %}
      {% endif %}
      <main>
        {% block content %}{% endblock %}
      </main>
      {% if section == 'en/index' or section == 'index' or section  == 'en/about-us' or section == 'en/practice-areas' or section == 'en/members' or section == 'en/contact' or section == 'en/careers' or section == 'en/practice-areas/civil-and-commercial-litigation' or section == 'en/practice-areas/constitutional-amparo-and-administrative-proceedings' or section == 'en/practice-areas/corporate' or section == 'en/practice-areas/foreign-investment' or section == 'en/practice-areas/immigration' or section == 'en/practice-areas/intellectual-property' or section == 'en/practice-areas/labor-and-employment' or section == 'en/practice-areas/mergers-and-acquisitions' or section == 'en/practice-areas/real-estate' or section == 'en/members/francisco-javier-padilla-mitre' or section == 'en/members/jaime-eduardo-martinez-terrazas' or section == 'en/members/Kristopher-ruben-ortiz-hernandez' or section == 'en/members/manuel-victorio-ortiz-chevaile' or section == 'en/members/mauricio-cristopher-arevalo-alcalde' or section == 'en/members/montserrat-del-rio-castaneda' or section == 'en/members/valeria-larrondo-tafoya' or section == 'en/members/vivian-ramirez-valladolid' %}
        {% include 'partials/footer-en.njk' %}
      {% else %}
        {% include 'partials/footer.njk' %}
      {% endif %}
    </div>
    {% block js_libs_config %}
    {% endblock %}
    <script src="/js/{{ projectFilesName }}.libs.js"></script>
    {% block js_project_config %}
    {% endblock %}
    <script src="/js/{{ projectFilesName }}.js"></script>
    {% block js_inline %}
    {% endblock %}

    <script src="//code.tidio.co/ugvcypcfaddua7v9cuifnybpno1jgxn1.js"></script>
  </body>
</html>
